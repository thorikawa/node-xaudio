{
  "name": "xaudio",
  "version": "0.0.0",
  "description": "Cross Platform node.js audio library powered by SDL/SDL_Mixer.",
  "author": {
  	"name": "Takahiro Poly Horikawa",
  	"email": "horikawa.takahiro@gmail.com"
  },
  "binary": {
